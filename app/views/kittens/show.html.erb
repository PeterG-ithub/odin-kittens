<% if flash[:notice] %>
  <div class="notice"><%= flash[:notice] %></div>
<% end %>
name: <%= @kitten.name%> <br>
age: <%= @kitten.age%> <br>
cuteness: <%= @kitten.cuteness%>/10 <br>
softness: <%= @kitten.softness%>/10 <br>

<%= link_to 'Home', kittens_path%>
<%= link_to 'Edit', edit_kitten_path%>
<%= link_to 'Delete', kitten_path(@kitten), 
    data: {turbo_method: :delete, turbo_confirm: "ARE YOU CERTAIN?" }%>